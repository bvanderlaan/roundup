<div id="userView" class="roundupform">
  <div>
    <h1><%= @user.to_s %></h1>
    <%= link_to 'Edit', edit_user_path(@user), { :class=>"button", :id=>"editButton"} %>
  </div>
  <div style="clear: both;"></div>
  <p id="notice"><%= notice %></p>
  <p>
    <strong>User name:</strong>
    <%= @user.user_name %>
  </p>
  <p>
    <strong>Email:</strong>
    <%= @user.email %>
  </p>
</div>